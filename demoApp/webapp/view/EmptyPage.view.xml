<!-- EmptyPage 空页面视图：
     1.xmlns 声明 xml 命名空间，sap.ui.layout、sap.ui.cor.mvc、sap.ui.core、sap.m,
            说明xmlns:mvc 其中 mvc 为别名，后续使用中会使用前缀(别名)加标签方式指定标签来源；
     2.controllerName 指定控制器文件，ZHAND_TR18_1001.controller.BaseController;
     3.MessagePage 标签使用；
-->
<mvc:View xmlns:mvc="sap.ui.core.mvc"
          xmlns="sap.m"
          controllerName="ZTEST_0005.controller.BaseController">
	<MessagePage id="messagePage"
				 title=""
				 text="{appProperties>/emptyPageText}"
				 icon="sap-icon://document"
				 description=""
				 showNavButton="{device>/system/phone}"
				 navButtonPress="onNavBack">
	</MessagePage>
</mvc:View>